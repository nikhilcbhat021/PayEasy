{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "paths": {
        "@/components/*": ["../../apps/user-app/components/*"],
        "@/data/*": ["../../data/*"],
        "@/layouts/*": ["../../layouts/*"],
        "@/lib/*": ["../../apps/user-app/lib/*"],
        "@/css/*": ["../../css/*"]
    },
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}

